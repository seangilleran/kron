{
  "tags":[
    "{{repeat(10)}}",
    {
      "tag":{
        "name":"{{company()}}"
      }
    }
  ],
  "posts":[
    "{{repeat(25)}}",
    {
      "post":{
        "title":"{{lorem(1, 'sentences')}}",
        "body":"{{lorem(3, 'paragraphs')}}",
        "tags":[
          "{{repeat(0, 4)}}",
          {
            "tag":{
              "id":"{{integer(1, 10)}}"
            }
          }
        ]
      }
    }
  ],
  "archives":[
    {
      "archive":{
        "name":"{{firstName()}} {{surname()}} Memorial Archive"
      }
    }
  ],
  "topics":[
    "{{repeat(15)}}",
    {
      "topic":{
        "name":"{{company()}} {{company()}}",
        "dates":"{{lorem(1, 'sentences')}}",
        "citations":"{{lorem(1, 'sentences')}}",
        "notes":"{{lorem(1, 'paragraphs')}}"
      }
    }
  ],
  "people":[
    "{{repeat(20)}}",
    {
      "person":{
        "name":"{{firstName()}} {{surname()}}",
        "dates":"{{lorem(1, 'sentences')}}",
        "citations":"{{lorem(1, 'sentences')}}",
        "notes":"{{lorem(1, 'paragraphs')}}",
        "topics":[
          "{{repeat(0, 5)}}",
          {
            "topic":{
              "id":"{{integer(1, 15)}}"
            }
          }
        ]
      }
    }
  ],
  "boxes":[
    "{{repeat(40)}}",
    {
      "box":{
        "number":"{{integer(1, 10000)}}",
        "archive":{
          "id":1
        },
        "people":[
          "{{repeat(0, 10)}}",
          {
            "person":{
              "id":"{{integer(1, 20)}}"
            }
          }
        ],
        "dates":"{{lorem(1, 'sentences')}}",
        "notes":"{{lorem(1, 'paragraphs')}}",
        "topics":[
          "{{repeat(0, 5)}}",
          {
            "topic":{
              "id":"{{integer(1, 15)}}"
            }
          }
        ]
      }
    }
  ],
  "documents":[
    "{{repeat(100)}}",
    {
      "document":{
        "title":"{{lorem(1, 'sentences')}}",
        "box":{
          "id":"{{integer(1, 40)}}"
        },
        "authors":[
          "{{repeat(1, 3)}}",
          {
            "person":{
              "id":"{{integer(1, 20)}}"
            }
          }
        ],
        "people":[
          "{{repeat(1, 3)}}",
          {
            "person":{
              "id":"{{integer(1, 20)}}"
            }
          }
        ],
        "dates":"{{lorem(1, 'sentences')}}",
        "citations":"{{lorem(1, 'sentences')}}",
        "notes":"{{lorem(1, 'paragraphs')}}",
        "topics":[
          "{{repeat(0, 5)}}",
          {
            "topic":{
              "id":"{{integer(1, 15)}}"
            }
          }
        ]
      }
    }
  ]
}